{
  "name": "endless-runners",
  "version": "1.0.0",
  "description": "Simple 2D game",
  "author": "sheldarr",
  "contributors": [
    {
      "name": "BartoszBaczek",
      "email": "bartosz.baczek@gmail.com"
    }
  ],
  "scripts": {
    "start": "nw .",
    "transpile": "webpack",
    "watch": "webpack --progress --color --watch"
  },
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sheldarr/endless-runners.git"
  },
  "bugs": {
    "url": "https://github.com/sheldarr/endless-runners/issues"
  },
  "window": {
    "title": "Endless Runners",
    "frame": true,
    "fullscreen": false,
    "toolbar": true,
    "resizeable": false,
    "width": 640,
    "height": 350,
    "min_width": 640,
    "max_width": 1600,
    "min_height": 350,
    "max_height": 900,
    "position": "center"
  },
  "chromium-args": "--enable-webgl --ignore-gpu-blacklist --disable-pinch",
  "dependencies": {
    "nw": "^0.24.1",
    "pixi.js": "^3.0.9",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "json-loader": "^0.5.4",
    "webpack": "^1.12.14"
  },
  "license": "MIT"
}
